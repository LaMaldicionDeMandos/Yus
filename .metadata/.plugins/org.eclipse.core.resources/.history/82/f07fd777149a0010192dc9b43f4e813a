package org.opensource.pasut.yus.configuration
{
	public class ProccessorsProxy
	{
		private var _proccessors:Array;
		public function ProccessorsProxy()
		{
			_proccessors = [];
		}
		
		public function get proccessors():Array{
			return _proccessors;
		}
		
		public function addProccessors(proccessors:Array):void{
			_proccessors = proccessors.concat(_proccessors);
		}
	}
}